{
  "name": "@zengbin/web-common-tools",
  "version": "1.0.0",
  "description": "Web前端公共基础 JavaScript 工具库",
  "main": "dist/web-common.js",
  "scripts": {
    "dev": "rollup --config scripts/rollup.config.dev.mjs  -w",
    "build": "rollup --config scripts/rollup.config.build.mjs",
    "lint": "npx eslint src",
    "lint:fix": "npx eslint src --fix",
    "generate-docs": "jsdoc -c jsdoc.json",
    "prepare": "husky install",
    "test": "jest --config scripts/jest.config.js"
  },
  "author": "bin.zeng",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "docdash": "^2.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^12.0.0",
    "prettier": "^3.0.3",
    "rollup": "^3.0.0",
    "rollup-watch": "^4.3.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  }
}
